            <type name="PREVIS"         path="SHOTS/{{assetBaseName}}/PREVIS"                  shotDependent="1"  environments="MAYA,HOUDINI"      output_path="RENDERED_IMAGES"/>
            <type name="LIGHTING"       path="SHOTS/{{assetBaseName}}/LIGHTING"                shotDependent="1"  environments="MAYA,HOUDINI"      output_path="RENDERED_IMAGES"/>
