            <type name="MATTE"          path="SHOTS/{{assetBaseName}}/PAINTINGS/MATTE_PAINT"   shotDependent="0"  environments="PHOTOSHOP"         output_path="MATTE_PAINT/OUTPUTS"/>
            <type name="TEXTURE"        path="ASSETS/{{assetBaseName}}/PAINTINGS/TEXTURES"     shotDependent="0"  environments="PHOTOSHOP"         output_path="TEXTURES"/>
