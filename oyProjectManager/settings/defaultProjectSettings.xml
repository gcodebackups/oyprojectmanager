                {{assetBaseName}}/CAMERAS
                {{assetBaseName}}/COMPOSITING
                {{assetBaseName}}/FX
                {{assetBaseName}}/EDIT/ONLINE
                {{assetBaseName}}/PREVIS
                {{assetBaseName}}/RENDERED_IMAGES
                {{assetBaseName}}/LIGHTING
                {{assetBaseName}}/SCENE_ASSEMBLY
                {{assetBaseName}}/MATCH_MOVE
            </shotDependent>
