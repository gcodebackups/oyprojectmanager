<?xml version="1.0" ?>
<root>
    <sequenceData shotPrefix="SH" shotPadding="3" revPrefix="r" revPadding="2" verPrefix="v" verPadding="3" timeUnit="pal" supportsTemplate="True">
        <structure>
            <shotDependent>
                SHOTS/{{assetBaseName}}/ANIMATIONS
                SHOTS/{{assetBaseName}}/CAMERAS
                SHOTS/{{assetBaseName}}/COMP
                SHOTS/{{assetBaseName}}/FX
                SHOTS/{{assetBaseName}}/EDIT/ONLINE
                SHOTS/{{assetBaseName}}/PREVIS
                SHOTS/{{assetBaseName}}/RENDERED_IMAGES
                SHOTS/{{assetBaseName}}/LIGHTING
                SHOTS/{{assetBaseName}}/SCENE_ASSEMBLY
                SHOTS/{{assetBaseName}}/MATCH_MOVE
            </shotDependent>
            <shotIndependent>
                PAINTINGS/ILLUSTRATION
                PAINTINGS/ILLUSTRATION/CHARACTER_DESIGNS
                PAINTINGS/MATTE_PAINT
                PAINTINGS/TEXTURES
                EDIT/OFFLINE
                EDIT/ONLINE/WHOLE_SEQUENCE
                EDIT/SOUND
                EDIT/ANIMATIC_STORYBOARD
                EDIT/MAKING_OF
                OTHERS
                OTHERS/assets
                OTHERS/clips
                OTHERS/data
                OTHERS/fur
                OTHERS/fur/furAttrMap
                OTHERS/fur/furEqualMap
                OTHERS/fur/furFiles
                OTHERS/fur/furImages
                OTHERS/fur/furShadowMap
                OTHERS/mel
                OTHERS/particles
                REFERENCES
                REFERENCES/ARTWORKS
                REFERENCES/TEXT
                REFERENCES/TEXT/SCENARIO
                REFERENCES/PHOTOS
                REFERENCES/STORYBOARD
            </shotIndependent>
        </structure>
        <assetTypes>
            <type name="ANIM"           path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="MAYA,HOUDINI"      output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="CAMERA"         path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="MAYA,HOUDINI"      output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="COMP"           path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="NUKE"              output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="EDIT"           path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="NUKE"              output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="FX"             path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="MAYA,HOUDINI"      output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="MODEL"          path="ASSETS/{{assetBaseName}}/{{assetTypeName}}"           shotDependent="0"  environments="MAYA,HOUDINI"      output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="OTHER"          path="OTHERS/{{assetBaseName}}/{{assetTypeName}}"           shotDependent="0"  environments="MAYA,HOUDINI,NUKE" output_path="OTHERS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="PREVIS"         path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="MAYA,HOUDINI"      output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="LIGHTING"       path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="MAYA,HOUDINI"      output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="RIG"            path="ASSETS/{{assetBaseName}}/{{assetTypeName}}"           shotDependent="0"  environments="MAYA,HOUDINI"      output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="SCENE_ASSEMBLY" path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="MAYA,HOUDINI"      output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="MATTE"          path="SHOTS/{{assetBaseName}}/PAINTINGS/{{assetTypeName}}"  shotDependent="0"  environments="PHOTOSHOP"         output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="TEXTURE"        path="ASSETS/{{assetBaseName}}/PAINTINGS/{{assetTypeName}}" shotDependent="0"  environments="PHOTOSHOP"         output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="ILLUSTRATION"   path="ASSETS/{{assetBaseName}}/PAINTINGS/{{assetTypeName}}" shotDependent="0"  environments="PHOTOSHOP"         output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="SHADING"        path="ASSETS/{{assetBaseName}}/{{assetTypeName}}"           shotDependent="0"  environments="MAYA,HOUDINI"      output_path="ASSETS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
            <type name="TRACKING"       path="SHOTS/{{assetBaseName}}/{{assetTypeName}}"            shotDependent="1"  environments="3DEQUALIZER"       output_path="SHOTS/{{assetBaseName}}/{{assetTypeName}}/OUTPUT/{{assetSubName}}"/>
        </assetTypes>
        <shotData>
        </shotData>
    </sequenceData>
</root>
